generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Session {
  id          String   @id
  userId      String
  title       String
  description String?  @db.VarChar(255)
  date        DateTime
  skill       String
  createdAt   DateTime @default(now())

  @@index([userId])
}
